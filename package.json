{
  "name": "my-app",
  "main": "index.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "bump:runtime": "node scripts/bump-runtime.mjs",
    "verify:runtime": "node scripts/verify-runtime-discipline.mjs",
    "verify:ai": "node scripts/verify-ai-standards.mjs",
    "verify:api": "node scripts/verify-api-contracts.mjs",
    "verify:reviews": "node scripts/verify-ai-reviews.mjs",
    "verify:native": "node scripts/verify-no-direct-native-imports.mjs",
    "verify:ui": "node scripts/verify-no-native-keyboard.mjs",
    "verify:fatal": "node scripts/verify-fatal.mjs",
    "collect:crash": "node scripts/collect-crash-logs.mjs",
    "verify:expo": "npx expo-doctor",
    "dev": "EXPO_NO_CLIENT_ENV=1 expo start --tunnel",
    "dev:mobile": "npx expo start --tunnel --clear",
    "start": "npm run verify:ai && expo start",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "all:dev": "npm run dev & npm run server:dev",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "web": "expo export --platform web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "db:push": "drizzle-kit push",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "build": "npm run verify:fatal && npm run verify:runtime && npm run verify:api && npm run verify:reviews && npm run verify:ai && npm run verify:native && npm run verify:ui && npm run verify:expo && npm run server:build && npm run web"
  },
  "dependencies": {
